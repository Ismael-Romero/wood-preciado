{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "wood-desktop",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http": {
        "all": true,
        "request": true,
        "scope": ["http://216.225.203.244:3000/*"]
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "theme": "Light",
        "title": "Maderer√≠a Preciado",
        "minWidth": 800,
        "minHeight": 600,
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.wood.desktop",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
